<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

        <bean id="fromBeanMessege" class="by.intexsoft.spring.Message">
            <property name="message" value="This is message from simple bean."/>
        </bean>
        <bean id="lazy" class="by.intexsoft.spring.Message" lazy-init="true">
        </bean>
        <bean id="init" class="by.intexsoft.spring.Message" init-method="getMessage">
        </bean>
        <bean id="destroyBean" class="by.intexsoft.spring.Message" destroy-method="getMessage">
        </bean>
</beans>
